#PASSWORD-GENERATOR_SHWETA SUNILKUMAR

import random
n = int(input("\nEnter number of characters: "))
ans = "y"
while(ans!="n"):
    password = ""
    for i in range(n):
        c = random.randint(33,126)
        password+=(chr(c))
    print("\n-> ", password)
    ans = input("Do you want another one (n for no)?")
print("Program terminated.")
