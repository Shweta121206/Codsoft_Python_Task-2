#Pasword-generator_Shweta Sunilkumar

import random
n = int(input("Enter number of characters: "))
ans = "y"
while(ans!="n"):
    password = ""
    for i in range(n):
        c = random.randint(33,126)
        password+=(chr(c))
    print(password)
    ans = input("Do you want another one (n for no)?")
print("Program terminated.")
